import java.io.*;
import java.util.*;
class rectangle
{
public static void main(String args[])
{
Scanner sc=new Scanner(System.in);
int i,j,rows,col;
System.out.println("Enter no.of rows:");
rows=sc.nextInt();
System.out.println("Enter no.of coloums:");
col=sc.nextInt();
for(i=1;i<=rows;i++)
{
System.out.println(" ");
for(j=1;j<=col;j++)
{
System.out.print("*");
}
}
}
}





