import java.io.*;
import java.util.*;
class righttriangle
{
public static void main(String args[])
{
Scanner sc=new Scanner(System.in);
int i,j,n;
System.out.println("Enter the number of rows:");
n=sc.nextInt();
for(i=0;i<=n;i++)
{
System.out.println(" ");
for(j=1;j<=i;j++)
{
System.out.print("*");
}
}
}
}
