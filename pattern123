import java.io.*;
import java.util.*;
class pattern123
{
public static void main(String args[])
{
Scanner sc=new Scanner(System.in);
int i,j,n;
System.out.println("Enter the number of rows:");
n=sc.nextInt();
for(i=1;i<=n;i++)
{
for(j=1;j<=i;j++)
{
System.out.print(i+ " ");
}
System.out.println();
}
}
}
